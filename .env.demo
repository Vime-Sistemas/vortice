# Demo environment variables for vortice
# Copy to .env and edit as needed
# Exemplos de backends (não aponte para a mesma porta do proxy)
BACKEND_URLS=http://localhost:8081,http://localhost:8082,http://localhost:8083
APP_PORT=8080

# Se quiser que o load-balancer inicie backends locais para desenvolvimento,
# habilite START_LOCAL_BACKENDS=true e ajuste a porta inicial e a quantidade.
# Quando START_LOCAL_BACKENDS=true os backends gerados serão adicionados aos
# BACKEND_URLS lidos.
START_LOCAL_BACKENDS=false
LOCAL_BACKEND_START_PORT=8081
LOCAL_BACKEND_COUNT=3
